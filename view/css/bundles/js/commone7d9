    hooks.months                = listMonths;
    hooks.isDate                = isDate;
    hooks.locale                = getSetGlobalLocale;
    hooks.invalid               = createInvalid;
    hooks.duration              = createDuration;
    hooks.isMoment              = isMoment;
    hooks.weekdays              = listWeekdays;
    hooks.parseZone             = createInZone;
    hooks.localeData            = getLocale;
    hooks.isDuration            = isDuration;
    hooks.monthsShort           = listMonthsShort;
    hooks.weekdaysMin           = listWeekdaysMin;
    hooks.defineLocale          = defineLocale;
    hooks.updateLocale          = updateLocale;
    hooks.locales               = listLocales;
    hooks.weekdaysShort         = listWeekdaysShort;
    hooks.normalizeUnits        = normalizeUnits;
    hooks.relativeTimeRounding  = getSetRelativeTimeRounding;
    hooks.relativeTimeThreshold = getSetRelativeTimeThreshold;
    hooks.calendarFormat        = getCalendarFormat;
    hooks.prototype             = proto;

    // currently HTML5 input type only supports 24-hour formats
    hooks.HTML5_FMT = {
        DATETIME_LOCAL: 'YYYY-MM-DDTHH:mm',             // <input type="datetime-local" />
        DATETIME_LOCAL_SECONDS: 'YYYY-MM-DDTHH:mm:ss',  // <input type="datetime-local" step="1" />
        DATETIME_LOCAL_MS: 'YYYY-MM-DDTHH:mm:ss.SSS',   // <input type="datetime-local" step="0.001" />
        DATE: 'YYYY-MM-DD',                             // <input type="date" />
        TIME: 'HH:mm',                                  // <input type="time" />
        TIME_SECONDS: 'HH:mm:ss',                       // <input type="time" step="1" />
        TIME_MS: 'HH:mm:ss.SSS',                        // <input type="time" step="0.001" />
        WEEK: 'GGGG-[W]WW',                             // <input type="week" />
        MONTH: 'YYYY-MM'                                // <input type="month" />
    };

    return hooks;

})));;
(function ($) {
    "use strict";
    $.fn.autocomplete = function autocomplete(options) {
        var $search = this,
            $options = options;

        $search.suggestions = [];
        $search.data = [];
        $search.input = $search.find("#search-input");
        $search.autocomplete = $search.find("#search-complete");
        $search.selectedIndex = -1;
        $search.hot = "[hot]";

        var hotGames = 6;
        var autoResults;
        var keys = {
            RETURN: 13,
            UP: 38,
            DOWN: 40
        };

        var messages = {
            NOMATCHING: "No matching results"
        };

        async function fetchGames(url) {
            let response = await fetch($options.baseUrl + url);
            if (response.ok) {
                return response.json();
            }
            return Error(response);
        }

        function setHotGames(games) {
            if (!games || games.length <= 0) return;
            $options.keywords = $search.hot;
            $search.autocomplete.html(renderGameList(games, $options.keywords));
            autoClick();
            setAuto(games, $options.keywords);
        }

        getHotGames();
        function getHotGames() {
            let _gamelistStorage = baseGetItems("search:games-hot");
            if (_gamelistStorage) {
                $search.data = _gamelistStorage.games;
            } else {
                fetchGames("/api/GameSearch/SearchGames")
                    .then(function (res) {
                        baseSetItems("search:games-hot", res ? res.Result : null);
                        $search.data = res ? res.Result : [];
                    })
                    .catch(function (err) {
                        $search.data = localStorage.getItem("search:games-hot").games;
                    });
            }
        }

        function gamesFilter(array, keywords) {
            let _keywords = keywords.toLowerCase();
            let _array = array;
            return _array
                .map(function (o) {
                    o.gameNameKeyIndex = o["GameName"].toLowerCase().indexOf(_keywords);
                    o.aliasKeyIndex = o["Aliases"].toLowerCase().indexOf(_keywords);
                    o.seoKeyIndex = o["SEO"].toLowerCase().indexOf(_keywords);
                    return o;
                })
                .filter(function (o) {
                    return o.gameNameKeyIndex >= 0 || o.aliasKeyIndex >= 0 || o.seoKeyIndex >= 0;
                })
                .map(function (o) {
                    o.sortIndex = (999 - (o.gameNameKeyIndex == -1 ? 999 : o.gameNameKeyIndex)) * 1000000 + (999 - (o.aliasKeyIndex == -1 ? 999 : o.aliasKeyIndex)) * 1000 + (999 - (o.seoKeyIndex == -1 ? 999 : o.seoKeyIndex)) * 1;
                    return o;
                })
                .sort(function (a, b) {
                    return b.sortIndex - a.sortIndex;
                });
        }

        function setGameSelected(index) {
            $($search.autocomplete.find(".item")[index || 0]).addClass("selected");
        }

        function searched() {
            let search_term = $.trim($search.input.val());
            if (search_term.length > 0) {
                $search.suggestions = gamesFilter($search.data, search_term);
                autoResults = $search.suggestions;
                $search.autocomplete.html(renderGameList($search.suggestions, search_term));
                setGameSelected();
                autoClick();
            } else {
                loadAuto();
            }
        }

        function showAuto(term) {
            let search_term = term;
            if (typeof term !== "string") {
                search_term = $search.input.val();
            }
            if (search_term.length > 0) {
                let _arr = $search.suggestions;
                $search.autocomplete.html(renderGameList(_arr, search_term));
                autoClick();
            } else {
                loadAuto();
            }

            $search.addClass("auto");
            $(".featured-games.dropdown").addClass("disabled");
            $search.input.attr("aria-controls", "search-inputautocomplete-list");
        }

        function loadAuto() {
            getHotGames();
            $search.suggestions = $search.data.slice(0, hotGames);
            setHotGames($search.suggestions || []);
        }

        function setAuto(arr) {
            if (arr && arr.length > 0) {
                autoResults = arr;
                $search.selectedIndex = -1;
            }
        }

        setAuto([""], true);

        function updateAutoScroll(index) {
            if (autoResults && autoResults.length >= 1) {
                let new_auto = autoResults[index]["GameName"];
                $search.input.val(new_auto);
                $search.autocomplete.find(".item").each(function (i) {
                    let $el = $(this);
                    if (index === i) {
                        $el.addClass("selected");
                        $el.attr("aria-expanded", "true");
                        let _thisItem = $el.find(".js-events-search");
                        _thisItem.attr("aria-selected", "true");
                        $("#search-input").attr("aria-activedescendant", _thisItem.attr("id"))
                    } else {
                        $el.removeClass("selected");
                        $el.attr("aria-expanded", "false");
                        $el.find(".js-events-search").attr("aria-selected", "false");
                    }
                });
            }
        }

        function entered() {
            $search.autocomplete.find(".item").each(function () {
                let $el = $(this);
                if (!$el.hasClass("selected")) {
                    return;
                }
                $el.find(".offer-product-type-tabs li").each(function (i) {
                    if (!$(this).attr("attr-data")) {
                        return;
                    }
                    let _href = $(this).attr("attr-href");
                    window.location = "/" + _href + "/";
                });
            });
        }

        function moveUp() {
            let that = $search;
            if (that.selectedIndex <= 0) {
                that.selectedIndex = $search.suggestions.length;
            }

            that.selectedIndex--;
            updateAutoScroll(that.selectedIndex);
        }

        function moveDOWN() {
            let that = $search;
            that.selectedIndex++;
            if (that.selectedIndex >= $search.suggestions.length) {
                that.selectedIndex = 0;
            }

            updateAutoScroll(that.selectedIndex);
        }

        function searchPressed(e) {
            switch (e.which) {
                case keys.RETURN:
                    entered();
                    break;
                case keys.UP:
                    moveUp();
                    break;
                case keys.DOWN:
                    moveDOWN();
                    break;
                default:
                    searched();
                    break;
            }
        }
        $search.input.on("keyup", searchPressed);
        $search.input.on("focus", showAuto);
        // $search.input.on('focusout', function disableSearchDropdown() {
        //     $search.removeClass('auto');
        //     $('.featured-games.dropdown').removeClass('disabled');
        // });

        function removeStorage(key) {
            try {
                localStorage.removeItem(key + "_expiresIn");
            } catch (e) {
                console.log("removeStorage: Error removing key [" + key + "] from localStorage: " + JSON.stringify(e));
                return false;
            }
            return true;
        }

        function getTimeStamp() {
            return Math.floor(Date.now() / 1000);
        }

        function getHours(timestamp) {
            let then = moment.unix(timestamp);
            let ms = moment(new Date(), "DD/MM/YYYY HH:mm:ss").diff(moment(then, "DD/MM/YYYY HH:mm:ss"));
            let d = moment.duration(ms).asHours();
            return Math.round(d);
        }

        function baseSetItems(key, obj) {
            if (!obj) return;
            let _now = getTimeStamp();
            let _obj = {
                timestamp: _now,
                games: obj
            };
            try {
                localStorage.setItem(key, JSON.stringify(_obj));
            } catch (e) {
                console.log("setStorage: Error setting key [" + key + "] in localStorage: " + JSON.stringify(e));
                return false;
            }
            return true;
        }

        function baseGetItems(key) {
            let _key = localStorage.getItem(key);
            if (!_key) return false;

            let _hours = getHours(JSON.parse(_key).timestamp);
            if (_hours >= 7) {
                removeStorage(key);
                return false;
            }
            try {
                return JSON.parse(_key);
            } catch (e) {
                console.log("getStorage: Error reading key [" + key + "] from localStorage: " + JSON.stringify(e));
                return false;
            }
        }

        function addSeachEventLog(e) {
            let _e = e;
            let search_term = $(_e).attr("attr-data").split(";");
            let _location = $(_e).attr("attr-href");
            let _id = $(_e).attr("id");

            if ($(window).width() <= 768) {
                if (!_id) return;
            }

            let _game = {
                UserSearchKeyWord: search_term[2],
                SearchGameId: search_term[0],
                SearchGameName: search_term[1]
            };

            $.ajax({
                type: "POST",
                url: $options.baseUrl + "/api/GameSearch/AddUserSearchGame",
                data: JSON.stringify(_game),
                contentType: "application/json; charset=utf-8",
                dataType: "json"
            }).always(function (res) {
                window.location = "/" + _location + "/";
            });
        }

        function autoClick() {
            $(".item,.js-events-search").on("click", function (e) {
                e.stopPropagation();
                addSeachEventLog($(this));
            });
        }

        function renderGameList(data, keywords) {
            if (!data) return;
            let _data = data;
            let _str = "";
            let itemList = '<div class="overflow-auto" id="search-inputautocomplete-list" role="listbox" aria-labelledby="pl-title">';
            _str += '<div class="autocomplete-items">'.concat(keywords === $search.hot ? '<div class="autocomplete-items-pl" id="pl-title"><p>popular games</p><hr/></div>' : "", itemList);

            _data.forEach(function (item, index) {
                _str += "<div "
                    .concat(item.GameName === keywords ? 'class="item selected"' : 'class="item"', ` aria-expanded="false" aria-controls="productTypeTab-${index + 1}" attr-data="`)
                    .concat(item.GameID, ";")
                    .concat(item.GameName, ";")
                    .concat(keywords, ";")
                    .concat(item.GameUrl, '" attr-href="')
                    .concat(item.SEO.toLowerCase() + '-marketplace', `">\n                    <a class="js-events-search" id="option-${index + 1}" role="option" tabindex="-1" aria-selected="false" attr-href="`)
                    .concat(item.SEO.toLowerCase() + '-marketplace', '" attr-data="')
                    .concat(item.GameID, ";")
                    .concat(item.GameName, ";")
                    .concat(keywords, '">\n                        ')
                    .concat(formatResult(item.GameName, keywords), "\n")
                    .concat(index <= 2 && keywords === $search.hot ? '<i class="iconfont icon-popular float-right"></i>' : "", `\n</a>\n<ul class="offer-product-type-tabs" id="productTypeTab-${index + 1}">\n`)
                    .concat(
                        item.HotGameProductType &&
                        item.HotGameProductType.map(function (ele) {
                            return "<li "
                                .concat(ele.DefaultProduct ? 'attr-data="'.concat(ele.DefaultProduct, '"') : "", ' attr-href="')
                                .concat(ele.ProductUrl, '"><a class="js-events-search" attr-href="')
                                .concat(ele.ProductUrl, '" id="')
                                .concat(item.GameName, '" attr-data="')
                                .concat(item.GameID, ";")
                                .concat(item.GameName, ";")
                                .concat(keywords, '">')
                                .concat(ele.DefaultProduct ? '<i class="iconfont icon-popular"></i>' : "")
                                .concat(ele.ProductName, "</a></li>");
                        }).join(""),
                        "</ul></div>"
                    );
            });

            _str += "</div>".concat(
                keywords !== $search.hot
                    ? '<div class="messages-pl">'.concat(
                        _data.length <= 0 ? messages.NOMATCHING : "",
                        '</div><div class="text-center"><hr/><div class="mb-10"><a class="txt-link" href="https://support.playerauctions.com/hc/en-us/requests/new?ticket_form_id=696888">Can\'t find what you\'re looking for? Request a game</a></div></div>'
                    )
                    : ""
            );
            _str += "</div>";
            return _str;
        }

        function formatResult(suggestion, currentValue) {
            if (!currentValue) {
                return suggestion;
            }

            let pattern = "(" + escapeRegExChars(currentValue) + ")";

            return suggestion
                .replace(new RegExp(pattern, "gi"), "<strong>$1</strong>")
                .replace(/&/g, "&amp;")
                .replace(/</g, "&lt;")
                .replace(/>/g, "&gt;")
                .replace(/"/g, "&quot;")
                .replace(/&lt;(\/?strong)&gt;/g, "<$1>");
        }

        function escapeRegExChars(value) {
            return value.replace(/[|\\{}()[\]^$+*?.]/g, "\\$&");
        }

        function hideResult(elmnt) {
            let _elmnt = $(elmnt).attr("id");
            let _input = $search.input.attr("id");
            if (_elmnt != _input) {
                $search.removeClass("auto");
                $(".featured-games.dropdown").removeClass("disabled");
            }
        }

        document.addEventListener("click", function (e) {
            hideResult(e.target);
        });
    };
})(jQuery);
;
